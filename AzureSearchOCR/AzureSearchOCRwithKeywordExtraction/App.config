<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <appSettings>
    <!--please put your suscription key here.-->
    <add key="textExtractionAccountKey" value=[Enter Azure ML Text Extraction API Key]/>
    <add key="oxfordSubscriptionKey" value=[Enter Project Oxford Vision Subscription Key]/>
    <add key="searchServiceName" value=[Enter Azure Search Search Service Name - Exclude .search.windows.net]/>
    <add key="searchServiceAPIKey" value=[Enter Azure Search Search Admin API Key]/>
  </appSettings>

    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-7.0.0.0" newVersion="7.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>